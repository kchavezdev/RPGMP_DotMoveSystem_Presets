    "@rollup/plugin-typescript": "^12.1.0",
    "@types/jasmine": "^3.10.6",
    "@types/node": "^14.18.21",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.32.0",
    "jasmine": "^3.99.0",
    "nyc": "^15.1.0",
